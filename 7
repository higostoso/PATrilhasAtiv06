using System;

namespace kgukjt
{
	class Program
	{
		public static void Main(string[] args)
		{
			
	
			
		 for (int e = 5; e <= 10; e++)
        {
            Console.WriteLine("Tabuada do {i}:");

            for (int d = 1; d <= 10; d++)
            {
              
                Console.WriteLine(e + "x" + d + "=" + e * d);
            }

            Console.WriteLine();
		

      
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
			
		   }
		}
	}
	
	}
